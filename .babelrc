{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    }
  },
  "plugins": [
    "transform-decorators-legacy",
    ["module-resolver", {
      "alias": {
        "@": "./src",
        "assets": "./assets"
      },
      "extensions": [".json", ".js"]
    }]
  ]
}
